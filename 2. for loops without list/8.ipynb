   ],
   "source": [
    "num1=int(input(\"enter the +ve number\"))\n",
    "print(num1)\n",
    "div_lst=[]\n",
    "for i in range(1,(num1//2)+1):\n",
    "    if num1%i==0:\n",
    "        div_lst.append(i)\n",
    "div_lst.append(num1)\n",
    "print(div_lst)"
